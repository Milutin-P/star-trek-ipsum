{
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [ "error", 2 ],
      "linebreak-style": [ "error", "unix" ],
      "semi": [ "error", "always" ]
    },
    "parserOptions": {
        "ecmaVersion": 2017
    }
  }